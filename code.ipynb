    "def clean_text(doc):\n",
    "    cleaned_tokens = [\n",
    "        token.lemma_.lower()  
    "        for token in doc\n",
    "        if not token.is_stop       
    "        and not token.is_punct       
    "        and not token.is_space       
    "        and len(token.text.strip()) > 1  
    "    ]\n",
    "    return \" \".join(cleaned_tokens)  "
   ]
