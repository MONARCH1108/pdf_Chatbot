    "def extract_text_from_pdf(pdf_path):\n",
    "    doc = fitz.open(pdf_path)\n",
    "    text = \"\"\n",
    "    for page in doc:\n",
    "        text += page.get_text(\"text\") + \"\\n\"\n",
    "    return text\n",
    "\n",
    "pdf_path = \"ur_pdf_file\"\n",
    "pdf_text = extract_text_from_pdf(pdf_path)\n",
    "doc = nlp(pdf_text)"
   ]
